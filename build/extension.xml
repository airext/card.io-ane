<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
  <id>com.yuppablee.cardscan</id>
  <versionNumber>1.0.0</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libCardScan.a</nativeLibrary>
        <initializer>YPLCardScanInitializer</initializer>
        <finalizer>YPLCardScanFinalizer</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>cardscan.jar</nativeLibrary>
        <initializer>com.yuppablee.cardscan.CardScan</initializer>
        <finalizer>com.yuppablee.cardscan.CardScan</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment />
    </platform>
  </platforms>
</extension>